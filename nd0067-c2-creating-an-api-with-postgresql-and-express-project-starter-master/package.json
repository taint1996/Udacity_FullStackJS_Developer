{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "Build Store Front Backend",
  "main": "server.ts",
  "scripts": {
    "start": "npm run delete && npm run build && node ./dest/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dest --onSuccess \"node ./dest/server.js\"",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint src/**/*.ts --fix",
    "build": "npx tsc",
    "delete": "del-cli ./dest",
    "test": "db-migrate db:drop storefront_product_test && db-migrate db:create storefront_product_test && db-migrate --env test up && ENV=test jasmine-ts && db-migrate --env test down && db-migrate db:drop storefront_product_test",
    "migrate": "db-migrate reset --env test && db-migrate up --env test"
  },
  "author": "Udacity - Nguyen Trung Tai (Tez)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "supertest": "^6.2.4",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^3.10.6",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.13",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.1",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.8.3"
  }
}
